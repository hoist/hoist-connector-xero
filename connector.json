{
  "name": "Xero",
  "slug" : "xero",
  "url": "http://www.xero.com",
  "auth_types" : {
    "Private": {
      "setup_help": "./samples/private-setup-help.md",
      "requires_signing" : false
    },
    "Public": {
      "setup_help": "./samples/public-setup-help.md",
      "requires_signing" : true
    },
   "Partner": {
      "setup_help": "./samples/partner-setup-help.md",
      "requires_signing" : true
    }
  },
  "settings": [{
    "key": "consumerKey",
    "type" : "text",
    "description": "Consumer Token",
    "for_types": ["Private", "Public"]
  },{
    "key": "consumerSecret",
    "type" : "text",
    "description": "Consumer Secret",
    "for_types": ["Private", "Public", "Partner"]
  },{
    "key": "publicKey",
    "type" : "longtext",
    "description": "Public Key",
    "for_types": ["Private"]
  },{
    "key": "privateKey",
    "type" : "hidden",
    "description": "Private Key",
    "for_types": ["Private"]
  }],
